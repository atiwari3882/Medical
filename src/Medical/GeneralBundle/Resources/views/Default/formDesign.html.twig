
{% block form_row %}
    <div class="row">
        <div class="label">
            {{ form_label(form) }}
        </div>
        <div class="field">
            {{ form_widget(form) }}
        </div>
        <div class="error">
            {{ form_errors(form) }}
        </div>
        <div class="stop"></div>
    </div>
{% endblock %}